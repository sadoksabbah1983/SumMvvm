<Window x:Class="SumMvvm.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SumMvvm"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="{Binding X}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="152,10,0,0" TextWrapping="Wrap" Text="{Binding Y}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="TextBoxZ" HorizontalAlignment="Left" Height="23" Margin="72,56,0,0" TextWrapping="Wrap" Text="{Binding Z}" VerticalAlignment="Top" Width="120" />
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="325,10,0,0" VerticalAlignment="Top" Width="95" Height="23" Command="{Binding SumCommand}"/>

    </Grid>
</Window>
